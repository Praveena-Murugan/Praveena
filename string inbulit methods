class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in); 
        System.out.print("Enter a string: ");
        String x = scanner.nextLine();          
            System.out.println(x.toUpperCase());       
            System.out.println(x.toLowerCase());       
        }
    }
