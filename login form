import java.util.Scanner;
public class LoginSystem {
    public static void main(String[] args) {
        String validUserId = "praveena";
        String validPassword = "praveena@1126";
        Scanner scanner = new Scanner(System.in);
        System.out.print("Enter User ID: ");
        String userId = scanner.nextLine();
        System.out.print("Enter Password: ");
        String password = scanner.nextLine();
        if (userId.equals(validUserId) && password.equals(validPassword)) {
            System.out.println("Login successful. Welcome, " + userId + "!");
        } else {
            System.out.println("Invalid user ID or password. Login failed.");
        }

        scanner.close();
    }
}
